	<!-- Databases -->
    <string name="db_create_alarms_table">CREATE TABLE IF NOT EXISTS alarms(_id INTEGER PRIMARY KEY, name TEXT, hour INTEGER DEFAULT 8, minute INTEGER DEFAULT 30, sunday INTEGER DEFAULT 0, monday INTEGER DEFAULT 1, tuesday INTEGER DEFAULT 1, wednesday INTEGER DEFAULT 1, thursday INTEGER DEFAULT 1, friday INTEGER DEFAULT 1, saturday INTEGER DEFAULT 0, content_uri TEXT);</string>
    <string name="db_create_notes_table">CREATE TABLE IF NOT EXISTS notes(_id INTEGER PRIMARY KEY, note TEXT, timestamp INTEGER);</string>
    <string name="db_create_tips_table">CREATE TABLE IF NOT EXISTS tips(_id INTEGER PRIMARY KEY, title TEXT, description TEXT, url TEXT);</string>

    <string name="db_update_alarms_add_enabled">ALTER TABLE alarms ADD COLUMN enabled INTEGER DEFAULT 1;</string>
    <string name="db_create_checklist_items_table">CREATE TABLE IF NOT EXISTS checklist_items(_id INTEGER PRIMARY KEY, name TEXT, category TEXT, enabled INTEGER DEFAULT 1);</string>
    <string name="db_create_checklist_events_table">CREATE TABLE IF NOT EXISTS checklist_events(_id INTEGER PRIMARY KEY, item_id INTEGER, timestamp INTEGER);</string>

    <string name="db_create_sleep_diary_table">CREATE TABLE IF NOT EXISTS sleep_diaries(_id INTEGER PRIMARY KEY, did_nap INTEGER DEFAULT 0, woke_earlier INTEGER DEFAULT 0, bed_hour INTEGER DEFAULT 12, bed_minute INTEGER DEFAULT 0, sleep_hour INTEGER DEFAULT 12, sleep_minute INTEGER DEFAULT 0, wake_hour INTEGER DEFAULT 12, wake_minute INTEGER DEFAULT 0, up_hour INTEGER DEFAULT 12, up_minute INTEGER DEFAULT 0, sleep_delay INTEGER DEFAULT 0,wake_count INTEGER DEFAULT 0, sleep_quality INTEGER DEFAULT 0, timestamp INTEGER);</string>
    <string name="db_update_sleep_diary_add_comments">ALTER TABLE sleep_diaries ADD COLUMN comments TEXT;</string>
    <string name="db_update_sleep_diary_add_rested">ALTER TABLE sleep_diaries ADD COLUMN rested INTEGER DEFAULT 0;</string>
    <string name="db_update_sleep_diary_add_nap_duration">ALTER TABLE sleep_diaries ADD COLUMN nap_duration TEXT;</string>
    <string name="db_update_sleep_diary_add_had_alcohol">ALTER TABLE sleep_diaries ADD COLUMN had_alcohol TEXT;</string>
    <string name="db_update_sleep_diary_add_alcohol_amount">ALTER TABLE sleep_diaries ADD COLUMN alcohol_amount TEXT;</string>
    <string name="db_update_sleep_diary_add_alcohol_time">ALTER TABLE sleep_diaries ADD COLUMN alcohol_time TEXT;</string>
    <string name="db_update_sleep_diary_add_had_caffeine">ALTER TABLE sleep_diaries ADD COLUMN had_caffeine TEXT;</string>
    <string name="db_update_sleep_diary_add_caffeine_amount">ALTER TABLE sleep_diaries ADD COLUMN caffeine_amount TEXT;</string>
    <string name="db_update_sleep_diary_add_caffeine_time">ALTER TABLE sleep_diaries ADD COLUMN caffeine_time TEXT;</string>
    
    <string name="db_create_sensor_readings_table">CREATE TABLE IF NOT EXISTS sensor_readings(_id INTEGER PRIMARY KEY, name TEXT, value REAL, recorded INTEGER);</string>
    
